menu title Debian Boot Options
timeout 100
default l0

label l0
        menu label Linux kernel @CONFIG_IMAGE_KERNEL_VER@
        kernel /vmlinuz-@CONFIG_IMAGE_KERNEL_VER@
        fdt /dtb-@CONFIG_IMAGE_KERNEL_VER@
        append rw console=ttyS0,115200 rootwait earlycon=sbi net.ifnames=0 root=/dev/mmcblk1p4

label l0-recovery
        menu label Linux kernel @CONFIG_IMAGE_KERNEL_VER@ (recovery mode)
        kernel /vmlinuz-@CONFIG_IMAGE_KERNEL_VER@
        fdt /dtb-@CONFIG_IMAGE_KERNEL_VER@
        append rw console=ttyS0,115200 rootwait earlycon=sbi net.ifnames=0 root=/dev/mmcblk1p4 single

label l1
        menu label Linux kernel 5.15-starfive-v2.11.5
        kernel /vmlinuz-5.15-starfive-v2.11.5
        fdt /dtb-5.15-starfive-v2.11.5
        append rw console=ttyS0,115200 rootwait earlycon=sbi net.ifnames=0 root=/dev/mmcblk1p4

label l1-recovery
        menu label Linux kernel 5.15-starfive-v2.11.5 (recovery mode)
        kernel /vmlinuz-5.15-starfive-v2.11.5
        fdt /dtb-5.15-starfive-v2.11.5
        append rw console=ttyS0,115200 rootwait earlycon=sbi net.ifnames=0 root=/dev/mmcblk1p4 single

label n0
        menu label Linux kernel @CONFIG_IMAGE_KERNEL_VER@ (NFS root)
        kernel /vmlinuz-@CONFIG_IMAGE_KERNEL_VER@
        fdt /dtb-@CONFIG_IMAGE_KERNEL_VER@
        append rw console=ttyS0,115200 rootwait earlycon=sbi net.ifnames=0 root=/dev/nfs ip=:::::eth0:dhcp nfsroot=@CONFIG_NFS_ROOT@,vers=4.2 nfsrootdebug

label n0-recovery
        menu label Linux kernel @CONFIG_IMAGE_KERNEL_VER@ (NFS root, recovery mode)
        kernel /vmlinuz-@CONFIG_IMAGE_KERNEL_VER@
        fdt /dtb-@CONFIG_IMAGE_KERNEL_VER@
        append rw console=ttyS0,115200 rootwait earlycon=sbi net.ifnames=0 root=/dev/nfs ip=:::::eth0:dhcp nfsroot=@CONFIG_NFS_ROOT@,vers=4.2 nfsrootdebug single


